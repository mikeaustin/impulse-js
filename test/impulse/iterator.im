class Iterator {
  constructor(range) {
    this._range = range;
    this._value = this._range._begin;
    //this._done  = false;
  }
  
  function next() {
    this._value = this._value + 1;
    this._done  = this._value >= this._range._end;

    return this;
  }
  
  function value() {
    return this._value;
  }
}

class Range {
  constructor(begin, end) {
    this._begin = begin - 1;
    this._end   = end;
  }

  function iterator() {
    var value = this._begin;
    
    return new Iterator(this);
  }
}

